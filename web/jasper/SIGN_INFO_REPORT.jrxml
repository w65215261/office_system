T.LAST_SIGN_ADDRESS AS '最后签到地址',
T.ORG_CNAME AS '组织机构ID',
T.ORG_CODE AS '组织机构编码'

FROM SIGN_INFO_REPORT T
WHERE T.LAST_SIGN_TIME BETWEEN $P{sDate} AND $P{eDate}
AND t.SIGN_PERSON_OID = $P{SIGN_PERSON_OID}]]>
